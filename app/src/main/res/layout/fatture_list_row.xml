<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:paddingTop="20dip"
	android:paddingRight="20dp"
	android:paddingLeft="20dp"
	android:id="@+id/fatturelistrowRelativeLayout1">

	<TextView
		android:id="@+id/fatture_listrow_data"
		android:layout_height="wrap_content"
		android:padding="2dip"
		android:textSize="13dip"
		android:textStyle="bold"
		android:layout_width="65dp"/>

	<TextView
		android:id="@+id/fatture_listrow_codice"
		android:layout_height="wrap_content"
		android:padding="0dip"
		android:textSize="13dip"
		android:textStyle="bold"
		android:layout_toRightOf="@+id/fatture_listrow_data"
		android:layout_width="80dp">

	</TextView>

	<TextView
		android:id="@+id/fatture_listrow_descrizione"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:padding="0dip"
		android:textSize="13dip"
		android:textStyle="bold"
		android:layout_toRightOf="@+id/fatture_listrow_codice"/>

	<TextView
		android:id="@+id/fatture_listrow_importo"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:padding="1dip"
		android:textSize="13dip"
		android:textStyle="bold"
		android:gravity="right"
		android:layout_alignParentRight="true">

	</TextView>

</RelativeLayout>

